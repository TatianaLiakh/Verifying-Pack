	else
	{
		sprintf(str,"%04d.c", File_Number);
	}
    strcat(string, str);
    Open_File_W(&fptr_C, string);
    GenHeadersInProc();
    Sem_Process();                /* Process */

    select_unit();                /* For } simbol */

    if (fptr_C != NULL) {
        fclose(fptr_C);            /* закроем файл NAMEproc.h*/
        fptr_C = NULL;
    }

#ifdef TEST_MODE_SEM
    printf("\n=== GEN_MAIN.C");
#endif
