	яняр йНМРПНКЭбЙКЧВЕМХЪмЮЯНЯЮ{
		еякх ( опнж бйк_мюяня_бюйсслмши б яняр оюяяхбмне){ 
			еякх (опнж бйк_мюяня_бюйсслмши б яняр ньхайю ) {
				ярюпр опнж бшй_мюяня_бюйсслмши; 
				ньхайю;
			}
			хмюве {
				ярюпр опнж йНМРПНКЭнРЙЮВЙХбЮССЛМНИрПСАШяХДЕПНЯРЮРЮ;	
				б якедсчыее;
			}
		} 
	} 
	
	яняр гЮОНЛХМЮЕЛдЮБКЕМХЕбБЮЙССЛМНИрПСАЕ{
			ко_оПЕДШДСЫЕЕ = то_P_бРПСАЕяХДЕП;
			б якедсчыее;
	}
		/* ЙНМРПНКЭ ОЕПЕЦПЕБЮ ПСАЮЬЙХ, ЯЮЛНОПНХГБНКЭМНЦН БШЙКЧВЕМХЪ БЕМРХКЪРНПЮ Х БНДЪМНЦН МЮЯНЯЮ*/
	яняр йНМРПНКЭнАНПСДНБЮМХЪ{
			еякх ( (то_T_бПСАЮЬЙЕ <= 85) && (й_бемрхкърнп) && (й_мюяня_нук) ) б якедсчыее;
			хмюве {
				ярюпр опнж бшй_мюяня_бюйсслмши;
			ньхайю;
			}
	}		
	яняр йНМРПНКЭдЮБКЕМХЪ{  
		еякх (то_P_бРПСАЕяХДЕП <= 0.02) {
			ярюпр опнж бшй_мюяня_бюйсслмши; 
			ярно;
			}
		хмюве б якедсчыее;
	} 	
	яняр оЮСГЮмЮнРЙЮВЙСбБЮЙССЛМНИрПСАЕ{  
		рюилюср дбе_лхмсрш б якедсчыее; 
	}  
	яняр оПНБЕПЙЮсЛЕМЭЬЕМХЪдЮБКЕМХЪбОЮРПСАЙЕ{  
		еякх(то_P_бРПСАЕяХДЕП < ко_оПЕДШДСЫЕЕ) б яняр гЮОНЛХМЮЕЛдЮБКЕМХЕбБЮЙССЛМНИрПСАЕ;
        хмюве ньхайю;		/* Л.А. ЯННАЫЕМХЕ БШДЮБЮРЭ, ВРН БЮЙССЛХПНБЮМХЧ РПСАШ - РПСАЮ */
	} 	
} /*ЙНМЕЖ ОПНЖЕЯЯЮ бйк_бюйсслхпнбюмхе_рпсаш */




